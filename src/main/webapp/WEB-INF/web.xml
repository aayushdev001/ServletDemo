<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>add</servlet-name>
    <servlet-class>com.aayush.AddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>add</servlet-name>
    <url-pattern>/add</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>sqr</servlet-name>
    <servlet-class>com.aayush.SquareServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>sqr</servlet-name>
    <url-pattern>/sqr</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ctx</servlet-name>
    <servlet-class>com.aayush.ContextServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ctx</servlet-name>
    <url-pattern>/ctx</url-pattern>
  </servlet-mapping>


  <context-param>
    <param-name>Name</param-name>
    <param-value>Aayush</param-value>
  </context-param>
  <context-param>
    <param-name>Mobile</param-name>
    <param-value>OnePlus</param-value>
  </context-param>
</web-app>
